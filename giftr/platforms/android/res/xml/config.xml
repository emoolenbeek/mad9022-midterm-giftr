<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.mool0008.myGiftr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>giftrApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <splash density="land-hdpi" src="res/screen/splash-land-hdpi.png" />
    <splash density="land-mdpi" src="res/screen/splash-land-mdpi.png" />
    <splash density="land-xhdpi" src="res/screen/splash-land-xhdpi.png" />
    <splash density="land-xxhdpi" src="res/screen/splash-land-xxhdpi.png" />
    <splash density="land-xxxhdpi" src="res/screen/splash-land-xxxhdpi.png" />
    <splash density="port-hdpi" src="res/screen/splash-port-hdpi.png" />
    <splash density="port-mdpi" src="res/screen/splash-port-mdpi.png" />
    <splash density="port-xhdpi" src="res/screen/splash-port-xhdpi.png" />
    <splash density="port-xxhdpi" src="res/screen/splash-port-xxhdpi.png" />
    <splash density="port-xxxhdpi" src="res/screen/splash-port-xxxhdpi.png" />
    <icon density="mdpi" src="res/icons/icon-mdpi.png" />
    <icon density="hdpi" src="res/icons/icon-hdpi.png" />
    <icon density="xhdpi" src="res/icons/icon-xhdpi.png" />
    <icon density="xxhdpi" src="res/icons/icon-xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icons/icon-xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
